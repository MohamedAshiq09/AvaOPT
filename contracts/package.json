{
  "name": "subnetyield-contracts",
  "version": "1.0.0",
  "description": "SubnetYield Core - Subnet contracts for cross-chain DeFi yield aggregation",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "test-network": "node test-network.js",
    "test-dfk": "node test-dfk.js",
    "compile": "hardhat compile",
    "test": "hardhat test --timeout 60000",
    "test:mock": "hardhat test test/MockDEXProtocol.test.ts",
    "test:yield": "hardhat test test/YieldScout.test.ts",
    "deploy:fuji": "hardhat run scripts/deploy.ts --network fuji",
    "deploy:fuji-backup1": "hardhat run scripts/deploy.ts --network fuji_backup1",
    "deploy:fuji-backup2": "hardhat run scripts/deploy.ts --network fuji_backup2",
    "deploy:fuji-backup3": "hardhat run scripts/deploy.ts --network fuji_backup3",
    "deploy:dfk": "hardhat run scripts/deploy.ts --network dfk",
    "deploy:dfk-testnet": "hardhat run scripts/deploy.ts --network dfk_testnet",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network mainnet",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "verify:fuji": "hardhat run scripts/verify.ts --network fuji",
    "verify:manual": "hardhat run scripts/manual-verify.ts --network fuji",
    "interact:fuji": "hardhat run scripts/interact.ts --network fuji",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "typechain": "hardhat typechain"
  },
  "keywords": [
    "defi",
    "avalanche",
    "cross-chain",
    "yield-farming",
    "awm",
    "teleporter"
  ],
  "author": "SubnetBlank Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^8.0.1",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
